{
  "name": "christianblos/symfony-di-annotation",
  "description": "Allows you to configure symfony dependency injections via annotations",
  "license": "MIT",
  "require": {
    "doctrine/annotations": "~1.0",
    "symfony/dependency-injection": ">=3.3.1",
    "symfony/config": ">=3.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\DependencyInjection\\Annotation\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "examples/"
    ]
  }
}
